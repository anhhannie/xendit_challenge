{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "qr_code_id",
    "description",
    "callback_url",
    "amount"
  ],
  "properties": {
    "qr_code_id": {
      "type": "string",
      "default": "",
      "title": "The qr_code_id Schema",
      "examples": [
        "XENDIT_QR_01"
      ]
    },
    "description": {
      "type": "string",
      "default": "",
      "title": "The description Schema",
      "examples": [
        "xendit challenge - Anh Pham"
      ]
    },
    "callback_url": {
      "type": "string",
      "default": "",
      "title": "The callback_url Schema",
      "examples": [
        "callback_link.com"
      ]
    },
    "amount": {
      "type": "integer",
      "default": 0,
      "title": "The amount Schema",
      "examples": [
        2500
      ]
    }
  },
  "examples": [
    {
      "qr_code_id": "XENDIT_QR_01",
      "description": "xendit challenge - Anh Pham",
      "callback_url": "callback_link.com",
      "amount": 2500
    }
  ]
}
